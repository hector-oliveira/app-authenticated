{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@components/*":  ["src/components/*"],
      "@interfaces/*":  ["src/global/interfaces/*"],
      "@themes/*":      ["src/global/themes/*"],
      "@screens/*":     ["src/screens/*"],
      "@appScreens/*":  ["src/screens/app/*"],
      "@authScreens/*": ["src/screens/auth/*"],
      "@routes/*":      ["./src/routes/*"],
    }
  }
}
